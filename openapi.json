        },
        "tags": [
          "produtores"
        ]
      },
      "delete": {
        "operationId": "ProdutoresController_delete",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "description": "ID do produtor",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Produtor deletado com sucesso"
          },
          "404": {
            "description": "Produtor não encontrado"
          }
        },
        "tags": [
          "produtores"
        ]
      },
      "get": {
