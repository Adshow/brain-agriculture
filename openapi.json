  "info": {
    "title": "Minha API",
    "description": "Documentação da API",
    "version": "1.0.0",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "components": {
    "schemas": {
      "CreateProdutorDto": {
        "type": "object",
        "properties": {
          "documento": {
            "type": "string",
            "example": "12345678900",
            "description": "CPF ou CNPJ do produtor"
          },
          "nome": {
            "type": "string",
            "example": "João Silva",
            "description": "Nome do produtor"
          }
        },
        "required": [
          "documento",
          "nome"
        ]
      },
      "Produtor": {
        "type": "object",
        "properties": {}
      },
      "UpdateProdutorDto": {
        "type": "object",
        "properties": {
          "nome": {
            "type": "string",
            "example": "João Silva Atualizado",
            "description": "Nome do produtor"
          },
          "cpfCnpj": {
            "type": "string",
            "example": "98765432100",
            "description": "Novo CPF/CNPJ do produtor"
          }
        }
      },
      "AddCulturasDto": {
        "type": "object",
        "properties": {
          "culturas": {
            "example": [
              "Soja",
              "Milho"
            ],
            "description": "Lista de culturas a serem adicionadas na safra",
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "culturas"
        ]
      },
      "Cultura": {
        "type": "object",
        "properties": {}
      },
      "CreatePropriedadeDto": {
        "type": "object",
        "properties": {
          "nome": {
            "type": "string",
            "example": "Fazenda Boa Vista",
            "description": "Nome da propriedade"
          },
          "cidade": {
            "type": "string",
            "example": "Luís Eduardo Magalhães",
            "description": "Cidade onde está localizada a propriedade"
          },
          "estado": {
            "type": "string",
            "example": "Bahia",
            "description": "Estado da propriedade"
          },
          "areaTotal": {
            "type": "number",
            "example": 150,
            "description": "Área total da propriedade em hectares"
          },
          "areaAgricultavel": {
            "type": "number",
            "example": 120,
            "description": "Área agricultável da propriedade em hectares"
          },
          "areaVegetacao": {
            "type": "number",
            "example": 30,
            "description": "Área de vegetação da propriedade em hectares"
          },
          "produtorId": {
            "type": "string",
            "example": "a8eb2f5c-24a9-459a-8c34-0d618efdb418",
            "description": "ID do produtor dono da propriedade"
          }
        },
        "required": [
          "nome",
          "cidade",
          "estado",
          "areaTotal",
          "areaAgricultavel",
          "areaVegetacao",
          "produtorId"
        ]
      },
      "Propriedade": {
        "type": "object",
        "properties": {}
      },
      "CreateSafraDto": {
        "type": "object",
        "properties": {
          "nome": {
            "type": "string",
            "example": "Safra de Soja 2025",
            "description": "Nome da safra"
          },
          "ano": {
            "type": "number",
            "example": 2025,
            "description": "Ano da safra"
          },
          "propriedadeId": {
            "type": "string",
            "example": "a8eb2f5c-24a9-459a-8c34-0d618efdb418",
            "description": "ID da propriedade vinculada à safra"
          }
        },
        "required": [
          "nome",
          "ano",
          "propriedadeId"
        ]
      },
      "Safra": {
        "type": "object",
        "properties": {}
      }
    }
  }
}